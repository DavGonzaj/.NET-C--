@model Chef

<h4 class="card-title text-center bg-dark text-light py-2 rounded-top">
    @Model.FirstName + @Model.LastName
</h4>
<p class="card-text" type="number">@Model.Birthday</p>

<form
  asp-controller="Chef"
  asp-action="DeleteChef"
  asp-route-dishId="@Model.ChefId"
  method="POST"
>
  <button class="btn btn-primary btn-danger">Delete</button>
</form>


